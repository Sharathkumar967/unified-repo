    "react": "*",
    "react-redux": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "redux-persist": "^6.0.0",
    "@repo/types": "workspace:*"
    
  }
